	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#767676",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#d4956c"
			}
		},
		{
			"name": "String escape characters",
			"scope": [
				"constant.character.escape",
				"constant.character.string.escape",
				"constant.regexp"
			],
			"settings": {
				"foreground": "#c19c00"
			}
		},
		{
			"name": "Template expression punctuation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#3b78ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#16c60c"
			}
		},
		{
			"name": "Constants and Built-ins",
			"scope": [
				"constant.language",
				"constant.language.boolean",
				"constant.language.null",
				"constant.language.undefined",
				"support.constant"
			],
			"settings": {
				"foreground": "#3b78ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Enums and constant members",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#61d6d6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords - Control Flow",
			"scope": [
				"keyword.control",
				"keyword.control.conditional",
				"keyword.control.loop",
				"keyword.control.flow",
				"keyword.control.return",
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#b4009e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords - Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#0037da",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords - Other",
			"scope": [
				"keyword",
				"keyword.other"
			],
			"settings": {
				"foreground": "#881798"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.bitwise"
			],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Operators - Special",
			"scope": [
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.instanceof",
				"keyword.operator.typeof",
				"keyword.operator.alignof",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#881798",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "#f9f1a5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function calls",
			"scope": [
				"variable.function",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#f9f1a5"
			}
		},
		{
			"name": "Class and Type names",
