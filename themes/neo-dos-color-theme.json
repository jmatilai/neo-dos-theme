				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#61d6d6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Type references",
			"scope": [
				"storage.type.cs",
				"storage.type.java",
				"storage.type.primitive",
				"storage.type.numeric.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#3a96dd"
			}
		},
		{
			"name": "Interfaces",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#7db8b8",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Inherited class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#61d6d6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"meta.definition.variable.name"
			],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Object properties",
